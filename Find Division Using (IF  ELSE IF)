/* Calculate Percentage Using (IF ELSE IF) Method - II*/
#include<stdio.h>
#include<conio.h>
int main()
{
    int m1, m2, m3, m4, m5, per;
    printf("\n Enter Marks if Five Subjects : ");
    scanf("%d %d %d %d %d", &m1, &m2, &m3, &m4, &m5);
    per = (m1+m2+m3+m4+m5)*100/500;

    if (per>=60)
        printf("First Division");
    else if (per>=50)
        printf("Second Division");
    else if (per>=40)
        printf("Third Division");
    else
        printf("Fail");
    return 0;
}